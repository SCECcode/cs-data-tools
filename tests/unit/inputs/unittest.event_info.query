select = distinct Ruptures.Source_ID,Ruptures.Rupture_ID,Rupture_Variations.Rup_Var_ID,Ruptures.Source_Name,Ruptures.Mag,Ruptures.Prob,Ruptures.Start_Lat,Ruptures.Start_Lon,Ruptures.End_Lat,Ruptures.End_Lon,Rupture_Variations.Hypocenter_Lat,Rupture_Variations.Hypocenter_Lon,Rupture_Variations.Hypocenter_Depth
from = CyberShake_Runs,CyberShake_Site_Ruptures,CyberShake_Sites,Rupture_Variations,Ruptures,Studies
where = CyberShake_Runs.ERF_ID=Rupture_Variations.ERF_ID and CyberShake_Runs.ERF_ID=Ruptures.ERF_ID and CyberShake_Runs.Rup_Var_Scenario_ID=Rupture_Variations.Rup_Var_Scenario_ID and CyberShake_Runs.Site_ID=CyberShake_Sites.CS_Site_ID and CyberShake_Runs.Study_ID=Studies.Study_ID and CyberShake_Site_Ruptures.CS_Site_ID=CyberShake_Sites.CS_Site_ID and CyberShake_Site_Ruptures.ERF_ID=Ruptures.ERF_ID and CyberShake_Site_Ruptures.Rupture_ID=Ruptures.Rupture_ID and CyberShake_Site_Ruptures.Source_ID=Ruptures.Source_ID and CyberShake_Sites.CS_Short_Name='USC' and Rupture_Variations.ERF_ID=Ruptures.ERF_ID and Rupture_Variations.Rupture_ID=Ruptures.Rupture_ID and Rupture_Variations.Source_ID=Ruptures.Source_ID and Ruptures.Mag>=7.0 and Ruptures.Mag<=7.5 and Ruptures.Source_Name LIKE '%Elsinore%' and Studies.Study_Name="Study 22.12 LF"
data_request_file = inputs/unittest.event_info.json
data_product = Event Info
