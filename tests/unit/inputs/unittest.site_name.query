where = CyberShake_Runs.Site_ID=CyberShake_Sites.CS_Site_ID and CyberShake_Runs.Study_ID=Studies.Study_ID and CyberShake_Sites.CS_Short_Name='USC' and Studies.Study_Name="Study 22.12 LF"
